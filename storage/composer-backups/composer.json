{
  "require": {
    "adigital/cookie-consent-banner": "2.0.0",
    "carlcs/craft-redactorcustomstyles": "4.0.3",
    "craftcms/cms": "^4.4.0",
    "craftcms/redactor": "3.0.4",
    "doublesecretagency/craft-cpcss": "2.6.0",
    "necoapps/craft-matrix-snap": "1.0.0",
    "nystudio107/craft-cookies": "4.0.0",
    "putyourlightson/craft-sprig": "2.6.2",
    "vaersaagod/matrixmate": "2.1.3",
    "verbb/cp-nav": "4.0.10",
    "verbb/smith": "2.0.0",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^1.3.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  }
}
